            var handle = function(info, city) {
                // info为remote_ip_info,city为与cookie中的city比较后的城市，cookie优先
                var province = info.province;
                city = info.city;
		//var province = shmIPlookup.getPNameByCName(city);
		//city = city;

                var desc = info.desc;
                if (info.ret != 1) {
                    return;
                 }

                var autoJS = '';
                var areaJS = '';
                var mtJS = '';
                switch (province) {
                   case '上海':
                        areaJS = '//www.sina.com.cn/dfz/js/2016/shanghai.js';
                        autoJS = '//sh.auto.sina.com.cn/cshh/ipposition-p1.d.html';
                        mtJS    = '//www.sina.com.cn/js/135/2013/ipmt/shanghai.js';
                        break;

                    case '四川':
                        areaJS = '//www.sina.com.cn/dfz/js/2016/sc.js';
                        autoJS = '//cd.auto.sina.com.cn/cshh/ipposition-p1.d.html';
                        mtJS    = '//www.sina.com.cn/js/135/2013/ipmt/sichuan.js';
                        break;

                    case '河南':
                        areaJS = '//www.sina.com.cn/dfz/js/2016/henan.js';
                        autoJS = '//zz.auto.sina.com.cn/cshh/ipposition-p1.d.html';
                        mtJS    = '//www.sina.com.cn/js/135/20 13/ipmt/henan.js';
                        break;

