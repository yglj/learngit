						areaJS = 'http://www.sina.com.cn/dfz/js/2016/nx.js';
						break;

                    default:
                        //autoJS = '';
                        //areaJS = '';
                        break;
                }
                // 汽车ip定向
                if(autoJS){
                    jsLoader({
                      url : autoJS,
                      charset:'gbk'
                    });
                }
                //地方站ip定向
                if(areaJS){
                    jsLoader({
                      url : areaJS,
                      charset:'utf-8'
                    });
                }
                //媒拓ip定向
                if(mt JS){
                    jsLoader({
                      url : mtJS,
                      charset:'gbk'
                    });
                }
            };

